{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "baseUrl": "."
  },
  "exclude": ["./node_modules", "./templates/**/*"],
  "include": ["./cli/**/*.ts"]
}
